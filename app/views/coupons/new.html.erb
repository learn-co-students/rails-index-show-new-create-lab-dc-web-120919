    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>New Coupon Page</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <h3>Coupon Form</h3>
        <%= form_tag coupons_path do %>
            <label>Coupon Code</label>
            <%= text_field_tag :'coupon[coupon_code]' %>
            <br>
            <label>Enter Store Name:</label>
            <%= text_field_tag :'coupon[store]' %>
            <br>
            <%= submit_tag "Submit Coupon"%>

        <% end %>
        <br>
        <%=link_to("Go back to index page", coupons_path ) %>
    </body>
</html>